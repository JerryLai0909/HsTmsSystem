<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent">


    <com.infinity.jerry.yyd_tms.ui.customview.ZTitlebar
        android:layout_width="match_parent"
        android:id="@+id/titleBar"
        android:layout_height="wrap_content">

    </com.infinity.jerry.yyd_tms.ui.customview.ZTitlebar>


    <com.infinity.jerry.yyd_tms.ui.customview.Z_SwipeListView
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:id="@+id/swipeListView"
        android:layout_height="0dp">

    </com.infinity.jerry.yyd_tms.ui.customview.Z_SwipeListView>

    <TextView
        style="@style/bottom_button"
        android:text="@string/addPoint"
        android:id="@+id/tvAddPoint" />

</LinearLayout>
